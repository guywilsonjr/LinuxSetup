alias py10='python3.10'
alias py10m='py10 -m'
alias pipinstall='pip install -U'
alias svenv='source .venv/bin/activate 2>/dev/null || source venv/bin/activate'
alias venvcrup='py10m venv --upgrade-deps --symlinks .venv'
alias venvupsert='venvcrup --clear'
alias venvupall='venvupsert && py10m installreqs'
alias installreqsof='pip install -U -r'
alias installreqs='installreqsof requirements.txt'
alias venvupall='venvupsert && py10m installreqs'
alias makevenvinstall='makevenv && installreqs'
alias bashrc='vim ~/.bashrc && source ~/.bashrc'
alias update='apt update && apt-file update && apt list --upgradable'
alias install='apt install -y'
alias upgrade='apt upgrade -y'
alias upperdate='update && upgrade'
alias list='apt list --upgradable'
alias gmain='git checkout main'
alias gmaster='git checkout master'
alias autoremove='apt autoremove'
alias purge='apt purge'
alias remove='apt remove'
alias vi='vim'
alias gsub='git submodule update --init'
alias gfet='git fetch'
alias gstat='git status'
alias gdiff='git diff'
alias gstash='git stash'
alias gpop='git stash pop'
alias gstatus='gfet && gstat'
alias gpull='git pull'
alias greb='git rebase'
alias glog='git log'
alias gam='git commit -a --amend'
alias gcheck='git checkout'
alias gpm='git push origin master || git push origin main'
alias gpush='git push'
alias gclone='git clone --recurse-submodules https://github.com/guywilsonjr/'
alias gbranch='git branch'


